/*
Write a program that takes Name, Address and phone number as input and gives output as
Name: blah
Adress: blah
Phone: blah
*/
#include<iostream>
#include<string>
using namespace std;

int main(){
    string name;
    string address;
    string phoneNumber;
    cout <<"Please enter your name: "<< endl;
    cin >> name;
    cout << "Please enter your address: " << endl;
    cin >> address;
    cout << "Please enter your phoneNumber: " << endl; 
    cin >> phoneNumber;
    cout << "Name is: " << name << endl;
    cout << "Address is: " << address << endl;
    cout << "Phone number is: " << phoneNumber << endl;

    return 0;

}
